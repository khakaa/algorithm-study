   "source": [
    "n = 12345\n",
    "\n",
    "def solution(n):\n",
    "    answer = [int(i) for i in str(n)]\n",
    "    answer.sort(reverse=True)\n",
    "    return answer\n",
    "\n",
    "solution(n)\n",
